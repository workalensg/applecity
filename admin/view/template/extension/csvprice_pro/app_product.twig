{{ header }}{{ column_left }}
<div id="content">
    <div class="page-header">
        <div class="container-fluid">
            <ul class="breadcrumb">
                {% for breadcrumb in breadcrumbs %}
                    <li><a href="{{ breadcrumb.href }}">{{ breadcrumb.text }}</a></li>
                {% endfor %}
            </ul>
        </div>
    </div>
    <div class="container-fluid csvprice_pro_container">
        {% if warning %}
            <div class="alert alert-danger"><i class="fa fa-exclamation-circle"></i> {{ warning }}
                <button type="button" class="close" data-dismiss="alert">&times;</button>
            </div>
        {% endif %}
        {% if success %}
            <div class="alert alert-success"><i class="fa fa-check-circle"></i> {{ success }}
                <button type="button" class="close" data-dismiss="alert">&times;</button>
            </div>
        {% endif %}
        <div class="csv-progress-info">
            <div id="progress-import" class="progress progress-striped active">
                <div class="progress-bar" role="progressbar" aria-valuenow="0" aria-valuemin="0"
                     aria-valuemax="100"></div>
            </div>
        </div>
        {{ app_header }}
        <div class="panel panel-default csvpricepro-form-wrapper">
            <div class="panel-body">
                <ul class="nav nav-tabs">
                    <li class="active">
                        <a href="#tab_setting" data-toggle="tab" id="link_tab_setting">{{ tab_setting }}</a></li>
                    <li><a href="#tab_fields" data-toggle="tab" id="link_tab_fields">{{ tab_fields }}</a></li>
                    <li><a href="#tab_export" data-toggle="tab" id="link_tab_export">{{ tab_export }}</a></li>
                    <li><a href="#tab_import" data-toggle="tab" id="link_tab_import">{{ tab_import }}</a></li>
                    <li><a href="#tab_macros" data-toggle="tab" id="link_tab_macros">{{ tab_macros }}</a></li>
                </ul>
                <div class="tab-content">
                    <div id="tab_setting" class="tab-pane active">
                        <div class="row">
                            <div class="col-sm-8 col-md-offset-2">
                                <div id="wrap_profile_setting" class="well well-sm">
                                    <form id="form_profile_setting" class="form-horizontal">
                                        <div class="form-group input-group-sm">
                                            <label class="col-sm-4 control-label"
                                                   data-prop_id="33">{{ text_profile_load }}</label>
                                            <div class="col-sm-8">
                                                <select name="profile_setting_select" id="profile_setting_select"
                                                        class="form-control input-sm"></select>
                                            </div>
                                        </div>
                                        <div class="form-group input-group-sm">
                                            <label class="col-sm-4 control-label">&nbsp;</label>
                                            <div class="col-sm-8">
                                                <button type="button" style="min-width:120px"
                                                        class="btn btn-info btn-sm"
                                                        onclick="loadProfile('profile_setting')">{{ button_load }}</button>
                                                <button type="button" style="min-width:120px"
                                                        class="btn btn-primary btn-sm"
                                                        onclick="updateProfile('profile_setting')">{{ button_save }}</button>
                                            </div>
                                        </div>
                                    </form>
                                </div>
                            </div>
                        </div>
                        <form action="{{ action }}" method="post" id="form_product_setting"
                              enctype="multipart/form-data" class="form-horizontal">
                            <div class="row">
                                <div class="col-sm-6">
                                    <div class="box-caption alert-info">
                                        <small>{{ text_default_product_setting }}</small>
                                    </div>
                                    <input type="hidden" name="form_product_setting_status" id="setting_default"
                                           value="1">
                                    <div class="form-group input-group-sm">
                                        <input type="hidden" name="csv_setting[to_store]" value="0">
                                        <label class="col-sm-5 control-label">{{ entry_store }}</label>
                                        <div class="col-sm-7">
                                            <div class="well well-sm" style="height: 150px; overflow: auto;">
                                                {% for store in stores %}
                                                    <div class="checkbox">
                                                        <label>
                                                            {% if store.store_id in csv_setting.to_store %}
                                                                <input type="checkbox" name="csv_setting[to_store][]"
                                                                       value="{{ store.store_id }}" checked="checked">
                                                                {{ store.name }}
                                                            {% else %}
                                                                <input type="checkbox" name="csv_setting[to_store][]"
                                                                       value="{{ store.store_id }}">
                                                                {{ store.name }}
                                                            {% endif %}
                                                        </label>
                                                    </div>
                                                {% endfor %}
                                            </div>
                                        </div>
                                    </div>
                                    <div class="form-group input-group-sm">
                                        <label class="col-sm-5 control-label">{{ entry_tax_class }}</label>
                                        <div class="col-sm-7">
                                            <select name="csv_setting[tax_class_id]" class="form-control input-sm">
                                                <option value="0">{{ text_none }}</option>
                                                {% for tax_class in tax_classes %}
                                                    {% if tax_class.tax_class_id == csv_setting.tax_class_id %}
                                                        <option value="{{ tax_class.tax_class_id }}"
                                                                selected="selected">{{ tax_class.title }}</option>
                                                    {% else %}
                                                        <option value="{{ tax_class.tax_class_id }}">{{ tax_class.title }}</option>
                                                    {% endif %}
                                                {% endfor %}
                                            </select>
                                        </div>
                                    </div>
                                    <div class="form-group input-group-sm">
                                        <label class="col-sm-5 control-label"
                                               data-prop_id="0">{{ entry_minimum }}</label>
                                        <div class="col-sm-7">
                                            <input class="form-control input-sm text-right" type="text"
                                                   name="csv_setting[minimum]" value="{{ csv_setting.minimum }}">
                                        </div>
                                    </div>
                                    <div class="form-group input-group-sm">
                                        <label class="col-sm-5 control-label">{{ entry_subtract }}</label>
                                        <div class="col-sm-7">
                                            <select name="csv_setting[subtract]" class="form-control input-sm">
                                                {% if csv_setting.subtract == 1 %}
                                                    <option value="1" selected="selected">{{ text_yes }}</option>
                                                    <option value="0">{{ text_no }}</option>
                                                {% else %}
                                                    <option value="1">{{ text_yes }}</option>
                                                    <option value="0" selected="selected">{{ text_no }}</option>
                                                {% endif %}
                                            </select>
                                        </div>
                                    </div>
                                    <div class="form-group input-group-sm">
                                        <label class="col-sm-5 control-label"
                                               data-prop_id="1">{{ entry_stock_status }}</label>
                                        <div class="col-sm-7">
                                            <select name="csv_setting[stock_status_id]" class="form-control input-sm">
                                                {% for stock_status in stock_statuses %}
                                                    {% if stock_status.stock_status_id == csv_setting.stock_status_id %}
                                                        <option value="{{ stock_status.stock_status_id }}"
                                                                selected="selected">{{ stock_status.name }}</option>
                                                    {% else %}
                                                        <option value="{{ stock_status.stock_status_id }}">{{ stock_status.name }}</option>
                                                    {% endif %}
                                                {% endfor %}
                                            </select>
                                        </div>
                                    </div>
                                    <div class="form-group input-group-sm">
                                        <label class="col-sm-5 control-label">{{ entry_shipping }}</label>
                                        <div class="col-sm-7">
                                            <select name="csv_setting[shipping]" class="form-control input-sm">
                                                {% if csv_setting.shipping == 1 %}
                                                    <option value="1" selected="selected">{{ text_yes }}</option>
                                                    <option value="0">{{ text_no }}</option>
                                                {% else %}
                                                    <option value="1">{{ text_yes }}</option>
                                                    <option value="0" selected="selected">{{ text_no }}</option>
                                                {% endif %}
                                            </select>
                                        </div>
                                    </div>
                                    <div class="form-group input-group-sm">
                                        <label class="col-sm-5 control-label">{{ entry_length_class }}</label>
                                        <div class="col-sm-7">
                                            <select name="csv_setting[length_class_id]" class="form-control input-sm">
                                                {% for length_class in length_classes %}
                                                    {% if length_class.length_class_id == csv_setting.length_class_id %}
                                                        <option value="{{ length_class.length_class_id }}"
                                                                selected="selected">{{ length_class.title }}</option>
                                                    {% else %}
                                                        <option value="{{ length_class.length_class_id }}">{{ length_class.title }}</option>
                                                    {% endif %}
                                                {% endfor %}
                                            </select>
                                        </div>
                                    </div>
                                    <div class="form-group input-group-sm">
                                        <label class="col-sm-5 control-label">{{ entry_weight_class }}</label>
                                        <div class="col-sm-7">
                                            <select name="csv_setting[weight_class_id]" class="form-control input-sm">
                                                {% for weight_class in weight_classes %}
                                                    {% if weight_class.weight_class_id == csv_setting.weight_class_id %}
                                                        <option value="{{ weight_class.weight_class_id }}"
                                                                selected="selected">{{ weight_class.title }}</option>
                                                    {% else %}
                                                        <option value="{{ weight_class.weight_class_id }}">{{ weight_class.title }}</option>
                                                    {% endif %}
                                                {% endfor %}
                                            </select>
                                        </div>
                                    </div>
                                    <div class="form-group input-group-sm">
                                        <label class="col-sm-5 control-label">{{ entry_layout }}</label>
                                        <div class="col-sm-7">
                                            {% for store in stores %}
                                                <p class="form-control-static">{{ store.name }}</p>
                                                <select name="csv_setting[layout][{{ store.store_id }}]"
                                                        class="form-control input-sm">
                                                    <option value="" selected="selected">&nbsp;</option>
                                                    {% for layout in layouts %}
                                                        {% if layout.layout_id == csv_setting.layout[store.store_id] %}
                                                            <option value="{{ layout.layout_id }}"
                                                                    selected="selected">{{ layout.name }}</option>
                                                        {% else %}
                                                            <option value="{{ layout.layout_id }}">{{ layout.name }}</option>
                                                        {% endif %}
                                                    {% endfor %}
                                                </select>
                                            {% endfor %}
                                        </div>
                                    </div>
                                    <div class="form-group input-group-sm">
                                        <label class="col-sm-5 control-label">{{ entry_sort_order }}</label>
                                        <div class="col-sm-7">
                                            <input class="form-control input-sm text-right" type="text"
                                                   name="csv_setting[sort_order]" value="{{ csv_setting.sort_order }}">
                                        </div>
                                    </div>
                                    <div class="form-group input-group-sm">
                                        <label class="col-sm-5 control-label">{{ entry_status }}</label>
                                        <div class="col-sm-7">
                                            <select name="csv_setting[status]" class="form-control input-sm">
                                                {% if csv_setting.status == 1 %}
                                                    <option value="1" selected="selected">{{ text_enabled }}</option>
                                                    <option value="0">{{ text_disabled }}</option>
                                                {% else %}
                                                    <option value="1">{{ text_enabled }}</option>
                                                    <option value="0" selected="selected">{{ text_disabled }}</option>
                                                {% endif %}
                                            </select>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-sm-6">
                                    <div class="box-caption alert-info">
                                        <small>{{ text_default_options_setting }}</small>
                                    </div>
                                    <div class="form-group input-group-sm">
                                        <label class="col-sm-5 control-label">{{ entry_option_type }}</label>
                                        <div class="col-sm-7">
                                            <select name="csv_setting[option_type]" class="form-control input-sm">
                                                <optgroup label="Choose">
                                                    <option value="select"{% if csv_setting.option_type is defined and csv_setting.option_type == 'select' %} selected="selected"{% endif %}>
                                                        Select
                                                    </option>
                                                    <option value="radio"{% if csv_setting.option_type is defined and csv_setting.option_type == 'radio' %} selected="selected"{% endif %}>
                                                        Radio
                                                    </option>
                                                    <option value="checkbox"{% if csv_setting.option_type is defined and csv_setting.option_type == 'checkbox' %} selected="selected"{% endif %}>
                                                        Checkbox
                                                    </option>
                                                    <option value="image"{% if csv_setting.option_type is defined and csv_setting.option_type == 'image' %} selected="selected"{% endif %}>
                                                        Image
                                                    </option>
                                                </optgroup>
                                                <optgroup label="Input">
                                                    <option value="text"{% if csv_setting.option_type is defined and csv_setting.option_type == 'text' %} selected="selected"{% endif %}>
                                                        Text
                                                    </option>
                                                    <option value="textarea"{% if csv_setting.option_type is defined and csv_setting.option_type == 'textarea' %} selected="selected"{% endif %}>
                                                        Textarea
                                                    </option>
                                                </optgroup>
                                                <optgroup label="File">
                                                    <option value="file"{% if csv_setting.option_type is defined and csv_setting.option_type == 'file' %} selected="selected"{% endif %}>
                                                        File
                                                    </option>
                                                </optgroup>
                                                <optgroup label="Date">
                                                    <option value="date"{% if csv_setting.option_type is defined and csv_setting.option_type == 'date' %} selected="selected"{% endif %}>
                                                        Date
                                                    </option>
                                                    <option value="time"{% if csv_setting.option_type is defined and csv_setting.option_type == 'time' %} selected="selected"{% endif %}>
                                                        Time
                                                    </option>
                                                    <option value="datetime"{% if csv_setting.option_type is defined and csv_setting.option_type == 'datetime' %} selected="selected"{% endif %}>
                                                        Date &amp; Time
                                                    </option>
                                                </optgroup>
                                            </select>
                                        </div>
                                    </div>
                                    <div class="form-group input-group-sm">
                                        <label class="col-sm-5 control-label">{{ entry_option_required }}</label>
                                        <div class="col-sm-7">
                                            <select name="csv_setting[option_required]" class="form-control input-sm">
                                                {% if csv_setting.option_required is defined and csv_setting.option_required == 1 %}
                                                    <option value="1" selected="selected">{{ text_yes }}</option>
                                                    <option value="0">{{ text_no }}</option>
                                                {% else %}
                                                    <option value="1">{{ text_yes }}</option>
                                                    <option value="0" selected="selected">{{ text_no }}</option>
                                                {% endif %}
                                            </select>
                                        </div>
                                    </div>
                                    <div class="form-group input-group-sm">
                                        <label class="col-sm-5 control-label">{{ entry_option_quantity }}</label>
                                        <div class="col-sm-7">
                                            <input class="form-control input-sm text-right" type="text"
                                                   name="csv_setting[option_quantity]"
                                                   value="{{ csv_setting.option_quantity ? csv_setting.option_quantity : 100 }}">
                                        </div>
                                    </div>
                                    <div class="form-group input-group-sm">
                                        <label class="col-sm-5 control-label">{{ entry_option_subtract_stock }}</label>
                                        <div class="col-sm-7">
                                            <select name="csv_setting[option_subtract_stock]"
                                                    class="form-control input-sm">
                                                {% if csv_setting.option_subtract_stock is defined and csv_setting.option_subtract_stock == 1 %}
                                                    <option value="1" selected="selected">{{ text_yes }}</option>
                                                    <option value="0">{{ text_no }}</option>
                                                {% else %}
                                                    <option value="1">{{ text_yes }}</option>
                                                    <option value="0" selected="selected">{{ text_no }}</option>
                                                {% endif %}
                                            </select>
                                        </div>
                                    </div>
                                    <div class="form-group input-group-sm">
                                        <label class="col-sm-5 control-label">{{ entry_option_price_prefix }}</label>
                                        <div class="col-sm-7">
                                            <select name="csv_setting[option_price_prefix]"
                                                    class="form-control input-sm">
                                                {% if csv_setting.option_price_prefix is defined and csv_setting.option_price_prefix == '-' %}
                                                    <option value="-" selected="selected">&nbsp;&nbsp;-&nbsp;&nbsp;
                                                    </option>
                                                    <option value="+">&nbsp;&nbsp;+&nbsp;&nbsp;</option>
                                                {% else %}
                                                    <option value="-">&nbsp;&nbsp;-&nbsp;&nbsp;</option>
                                                    <option value="+" selected="selected">&nbsp;&nbsp;+&nbsp;&nbsp;
                                                    </option>
                                                {% endif %}
                                            </select>
                                        </div>
                                    </div>
                                    <div class="form-group input-group-sm">
                                        <label class="col-sm-5 control-label">{{ entry_option_points_prefix }}</label>
                                        <div class="col-sm-7">
                                            <select name="csv_setting[option_points_prefix]"
                                                    class="form-control input-sm">
                                                {% if csv_setting.option_points_prefix is defined and csv_setting.option_points_prefix == '-' %}
                                                    <option value="-" selected="selected">&nbsp;&nbsp;-&nbsp;&nbsp;
                                                    </option>
                                                    <option value="+">&nbsp;&nbsp;+&nbsp;&nbsp;</option>
                                                {% else %}
                                                    <option value="-">&nbsp;&nbsp;-&nbsp;&nbsp;</option>
                                                    <option value="+" selected="selected">&nbsp;&nbsp;+&nbsp;&nbsp;
                                                    </option>
                                                {% endif %}
                                            </select>
                                        </div>
                                    </div>
                                    <div class="form-group input-group-sm">
                                        <label class="col-sm-5 control-label">{{ entry_option_points_default }}</label>
                                        <div class="col-sm-7">
                                            <input class="form-control input-sm text-right" type="text"
                                                   name="csv_setting[option_points_default]"
                                                   value="{{ csv_setting.option_points_default ? csv_setting.option_points_default }}">
                                        </div>
                                    </div>
                                    <div class="form-group input-group-sm">
                                        <label class="col-sm-5 control-label">{{ entry_option_price_prefix }}</label>
                                        <div class="col-sm-7">
                                            <select name="csv_setting[option_weight_prefix]"
                                                    class="form-control input-sm">
                                                {% if csv_setting.option_weight_prefix is defined and csv_setting.option_weight_prefix == '-' %}
                                                    <option value="-" selected="selected">&nbsp;&nbsp;-&nbsp;&nbsp;
                                                    </option>
                                                    <option value="+">&nbsp;&nbsp;+&nbsp;&nbsp;</option>
                                                {% else %}
                                                    <option value="-">&nbsp;&nbsp;-&nbsp;&nbsp;</option>
                                                    <option value="+" selected="selected">&nbsp;&nbsp;+&nbsp;&nbsp;
                                                    </option>
                                                {% endif %}
                                            </select>
                                        </div>
                                    </div>
                                    <div class="form-group input-group-sm">
                                        <label class="col-sm-5 control-label">{{ entry_option_weight_default }}</label>
                                        <div class="col-sm-7">
                                            <input class="form-control input-sm text-right" type="text"
                                                   name="csv_setting[option_weight_default]"
                                                   value="{{ csv_setting.option_weight_default ? csv_setting.option_weight_default }}">
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </form>
                    </div><!-- END tab_setting -->

                    <div id="tab_fields" class="tab-pane fade">
                        <div class="row">
                            <div class="col-sm-8 col-md-offset-2">
                                <div id="wrap_profile_fields" class="well well-sm">
                                    <form id="form_profile_fields" class="form-horizontal">
                                        <div class="form-group input-group-sm">
                                            <label class="col-sm-4 control-label"
                                                   data-prop_id="33">{{ text_profile_load }}</label>
                                            <div class="col-sm-8">
                                                <select name="profile_fields_select" id="profile_fields_select"
                                                        class="form-control input-sm"></select>
                                            </div>
                                        </div>
                                        <div class="form-group input-group-sm">
                                            <label class="col-sm-4 control-label">&nbsp;</label>
                                            <div class="col-sm-8">
                                                <button type="button" style="min-width:120px"
                                                        class="btn btn-info btn-sm"
                                                        onclick="loadProfile('profile_fields')">{{ button_load }}</button>
                                                <button type="button" style="min-width:120px"
                                                        class="btn btn-primary btn-sm"
                                                        onclick="updateProfile('profile_fields')">{{ button_save }}</button>
                                            </div>
                                        </div>
                                    </form>
                                </div>
                            </div>
                        </div>
                        <form action="{{ action }}" method="post" id="form-product-field-setting"
                              enctype="multipart/form-data" class="form-horizontal">
                            <input type="hidden" name="formProductFieldSetting" value="1">
                            <div class="row">
                                <div class="col-sm-12" style="padding-left:20px;padding-bottom:8px">
                                    <div class="form-inline"
                                         style="padding-left:10px;padding-bottom:8px;border-bottom:1px solid #ddd;">
                                        <div class="form-group input-group-sm" style="margin:0">
                                            <label class="control-label" style="padding-right:10px"
                                                   data-prop_id="35">{{ entry_find_by_column_number }}</label>
                                            <select name="productFieldNameAsNumber" class="form-control input-sm"
                                                    style="min-width:120px">
                                                {% if productFieldNameAsNumber is defined and productFieldNameAsNumber == 1 %}
                                                    <option value="1" selected="selected">{{ text_yes }}</option>
                                                    <option value="0">{{ text_no }}</option>
                                                {% else %}
                                                    <option value="1">{{ text_yes }}</option>
                                                    <option value="0" selected="selected">{{ text_no }}</option>
                                                {% endif %}
                                            </select>
                                        </div>
                                    </div>
                                    <script>
                                        $('select[name=productFieldNameAsNumber]').change(function () {
                                            if ($(this).val() == 1) {
                                                $('.csv-field-name-number').text('{{ entry_csv_number }}');
                                            } else {
                                                $('.csv-field-name-number').text('{{ entry_csv_name }}');
                                            }
                                        });
                                    </script>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-sm-6">
                                    <table id="table-product-field" class="table table-hover table-striped">
                                        <thead>
                                        <tr>
                                            <th>{{ entry_field_name }}</th>
                                            {% if productFieldNameAsNumber is defined and productFieldNameAsNumber == 1 %}
                                                <th class="csv-field-name-number">{{ entry_csv_number }}</th>
                                            {% else %}
                                                <th class="csv-field-name-number">{{ entry_csv_name }}</th>
                                            {% endif %}
                                            <th>&nbsp;</th>
                                        </tr>
                                        </thead>
                                        {% set product_field_row = 0 %}
                                        {% if productFieldNameSetting is not empty %}
                                            <tbody id="tbody_data-field">
                                            {% for field_key, filed_name in productFieldNameSetting %}
                                                <tr id="product-field-row{{ product_field_row }}">
                                                    <td class="text-left" width="46%">
                                                        <input type="hidden"
                                                               name="productFieldNameSetting[{{ field_key }}]"
                                                               value="{{ filed_name }}">{{ field_key }}
                                                    </td>
                                                    <td class="text-left">{{ filed_name }}</td>
                                                    <td class="text-center">
                                                        <a onclick="$('#product-field-row{{ product_field_row }}').remove();"
                                                           data-toggle="tooltip" title="" class="btn btn-danger btn-sm"
                                                           data-original-title="{{ button_remove }}"><i
                                                                    class="fa fa-minus-circle"></i></a>
                                                    </td>
                                                </tr>
                                                {% set product_field_row = product_field_row + 1 %}
                                            {% endfor %}
                                            </tbody>
                                            <tbody id="tbody_foot-field"></tbody>
                                        {% else %}
                                            <tbody id="tbody_foot-field">
                                            <tr>
                                                <td colspan="3">
                                                    {{ text_no_results }}
                                                </td>
                                            </tr>
                                            </tbody>
                                        {% endif %}
                                        <tbody id="tbody_form-field" class="csvpricepro_tbody-form">
                                        <tr>
                                            <td>
                                                <select id="product_field_select" class="form-control input-sm">
                                                    {% for fieldName in productFieldNames %}
                                                        <option value="{{ fieldName }}">{{ fieldName }}</option>
                                                    {% endfor %}
                                                </select>
                                            </td>
                                            <td>
                                                <input type="text" id="product_field_input" value=""
                                                       class="form-control input-sm">
                                            </td>
                                            <td width="50" class="text-center">
                                                <a onclick="addProductFieldRow();" data-toggle="tooltip" title=""
                                                   class="btn btn-success btn-sm"
                                                   data-original-title="{{ button_insert }}"><i
                                                            class="fa fa-plus-circle"></i></a>
                                            </td>
                                        </tr>
                                        </tbody>
                                    </table>
                                </div>
                                <div class="col-sm-6">
                                    <table id="table-product-attribute-field" class="table table-hover table-striped">
                                        <thead>
                                        <tr>
                                            <th>{{ entry_attribute_name }}</th>
                                            {% if productFieldNameAsNumber is defined and productFieldNameAsNumber == 1 %}
                                                <th class="csv-field-name-number">{{ entry_csv_number }}</th>
                                            {% else %}
