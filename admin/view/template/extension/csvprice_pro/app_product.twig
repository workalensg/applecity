            getProfile('profile_import', {{ profile_id ? profile_id : 0 }});
            getProfile('profile_setting', {{ profile_id ? profile_id : 0 }});
            getProfile('profile_fields', {{ profile_id ? profile_id : 0 }});
        });
        jQuery(document).ready(function ($) {
            var s = {{ processing_import }};
            if (s) {
                $('.alert').remove();
                $('#progress-import').parent().hide();
                $('#progress-import .progress-bar').attr('aria-valuenow', 1);
                $('#progress-import .progress-bar').css('width', '1%');
                $('#progress-import').parent().after('<div class="alert alert-info" style="color: #333"><i class="fa fa-check-circle"></i> {{ text_begin_auto_import }} </div>');
                $('#progress-import').parent().show();
                processingImport(0);
            } else {
                $('#progress-import').parent().hide();
            }
        });

        function processingImport(p, f = 0) {
            $.ajax({
                type: 'POST',
                url: '{{ action_processing_import }}&p=' + p + '&f=' + f,
                data: {'progress': p},
                dataType: 'json',
                success: function (json) {
                    if (json.processed == 0) {
                        $('.alert').remove();
                        $('#progress-import').parent().hide();
                        $('#progress-import').parent().after('<div class="alert alert-danger" style="color: #333"><i class="fa fa-exclamation-circle"></i> ' + json.message + ' <button type="button" class="close" data-dismiss="alert">&times;</button></div>');
                        return false;
                    }
                    if (json.status == 100) {
                        $('.alert').remove();
                        $('#progress-import .progress-bar').css('width', '100%').attr('aria-valuenow', 100);
                        $('#progress-import').parent().after('<div class="alert alert-success" style="color: #333"><i class="fa fa-check-circle"></i> ' + json.message + '</div>');
                        $('#progress-import').removeClass("active");
                        //$('#progress-import .progress-bar span').text('100%');
                    } else {
                        $('.alert').remove();
                        $('#progress-import .progress-bar').css('width', json.status + '%').attr('aria-valuenow', json.status);
                        $('#progress-import').parent().after('<div class="alert alert-info" style="color: #333"><i class="fa fa-check-circle"></i> ' + json.message + '</div>');
                        processingImport(json.status);
                    }
                }, error: function (xhr, ajaxOptions, thrownError) {
                    alert(thrownError + "\r\n" + xhr.statusText + "\r\n" + xhr.responseText);
                }
            });
        }

        var product_exclude_field_row = {{ product_exclude_field_row }};

        function addProductExcludeFieldRow() {
            var field = $('#product_exclude_field_select option:selected').val();
            var html = '<div id="product-exclude-field-row' + product_exclude_field_row + '" class="input-group csvprice_pro-exclude-item">';
            html += '<input style="width:auto" type="text" name="csv_import[exclude_field][]" readonly class="form-control input-sm" value="' + field + '">';
            html += '<div class="input-group-btn"><button type="button" onclick="$(\'#product-exclude-field-row' + product_exclude_field_row + '\').remove();" data-toggle="tooltip" title="" class="btn btn-danger btn-sm" data-original-title="{{ button_remove }}"><i class="fa fa-minus-circle"></i></button></div>';
            html += '</div>';
            if (product_exclude_field_row < 1) {
                $('#exclude-field-list').html('');
            }
            $('#exclude-field-list').append(html);
            product_exclude_field_row++;
        }

        var productAttributeFieldRow = {{ productAttributeFieldRow ? productAttributeFieldRow : 0 }};
        $('#input-product-attribute-select').autocomplete({
            'source': function (request, response) {
                $.ajax({
                    url: 'index.php?route=catalog/attribute/autocomplete&user_token={{ user_token }}&filter_name=' + encodeURIComponent(request),
                    dataType: 'json',
                    success: function (json) {
                        response($.map(json, function (item) {
                            return {
                                category: item.attribute_group,
                                label: item.name,
                                value: item.attribute_id
                            }
                        }));
                    }
                });
            },
            'select': function (item) {
                $('#input-product-attribute-select').val(item['category'] + ': ' + item['label']);
                $('#input-product-attribute-value').val(item['value']);
            }
        });

        function addProductAttributeFieldRow() {
            var attribute_name = $('#input-product-attribute-select').val(),
                attribute_id = $('#input-product-attribute-value').val(),
                csv_name = $('#input-product-attribute-csv-name').val();
            if (csv_name == '' || attribute_id <= 0) return;
            var html = '<tr id="product-attribute-field-row' + productAttributeFieldRow + '">';
            html += '<td class="text-left" width="46%"><input type="hidden" name="productAttributeFieldNameSetting[' + attribute_id + '][id]" value="' + attribute_id + '">' + attribute_name + '</td>';
            html += '<td class="text-left"><input type="hidden" name="productAttributeFieldNameSetting[' + attribute_id + '][csv_name]" value="' + csv_name + '">' + csv_name + '</td>';
            html += '<td class="text-center"><a onclick="$(\'#product-attribute-field-row' + productAttributeFieldRow + '\').remove();" data-toggle="tooltip" title="" class="btn btn-danger btn-sm" data-original-title="{{ button_remove }}"><i class="fa fa-minus-circle"></i></a>';
            html += '</tr>';
            if (productAttributeFieldRow < 1) {
                $('#table-product-attribute-field #tbody-product-attribute-field').html('');
            }
            $('#table-product-attribute-field #tbody-product-attribute-field').append(html);
            $('#input-product-attribute-select').val('');
            $('#input-product-attribute-value').val(0);
            $('#input-product-attribute-csv-name').val('');
            productAttributeFieldRow++;
        }
    </script>
    {{ app_footer }}
</div>
{{ footer }}